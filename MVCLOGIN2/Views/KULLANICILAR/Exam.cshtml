@model IEnumerable<MVCLOGIN2.Models.take>

@{
    ViewBag.Title = "Exam";
}


@Html.Partial("_StudentInfo")
<div class="row">
    <div class="container">
        
        <div class="row">
            <div class="container">
                <div class="container col-lg-6" style="margin-left:5rem">
                    <h2>Exam</h2>
                    <table class="table">
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.cours.coursename)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.cours.room)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.cours.examdate)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.cours.examtime)
                            </th>
                            <th></th>
                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @Html.DisplayFor(modelItem => item.cours.coursename)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.cours.room)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.cours.examdate)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.cours.examtime)
                                </td>
                            </tr>
                        }

                    </table>
                </div>
            </div>

        </div>

    </div>
</div>
<div class="col-lg-12" style="margin-top:3rem">
    @Html.Partial("_LeftBar")


</div>
